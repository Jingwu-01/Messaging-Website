  "properties": {
    "path": {
      "type": "string"
    },
    "doc": {
      "type": "object"
    },
    "meta": {
      "type": "object",
      "required": [
        "createdAt",
        "createdBy",
        "lastModifiedAt",
        "lastModifiedBy"
      ],
      "properties": {
        "createdAt": {
          "type": "integer"
        },
        "createdBy": {
          "type": "string"
        },
        "lastModifiedAt": {
          "type": "integer"
        },
        "lastModifiedBy": {
          "type": "string"
        }
      }
    }
  }
}
